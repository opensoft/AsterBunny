{
    "name": "opensoft/asterbunny",
    "description": "This tool sends json encoded asterisk ami events to RabbitMQ",
    "keywords": ["rabbitmq", "asterisk", "pami", "ami", "amqp"],
    "license": "MIT",
    "authors": [
        {
            "name": "Richard Fullmer",
            "email": "richard.fullmer@opensoftdev.com"
        }
    ],
    "repositories": [
        {
            "type": "pear",
            "url": "http://pear.apache.org/log4php/"
        },
        {
            "type": "package",
            "package": {
                "name": "marcelog/pami",
                "version": "dev-master",
                "dist": {
                    "url": "https://github.com/macerlog/PAMI/zipball/master",
                    "type": "zip"
                },
                "source": {
                    "url": "git://github.com/marcelog/PAMI.git",
                    "type": "git",
                    "reference": "master"
                },
                "require": {
                    "pear-log4php/Apache_log4php": ">=2.1.0"
                },
                "autoload": {
                    "psr-0": { "PAMI": "src/mg/" }
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/console": "dev-master",
        "videlalvaro/php-amqplib": "dev-master",
        "marcelog/pami": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Opensoft\\AsterBunny": "src/"
        }
    },
    "bin": [
        "bin/asterbunny"
    ]
}
