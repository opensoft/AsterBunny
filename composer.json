{
    "name": "opensoft/asterbunny",
    "description": "This tool sends json encoded asterisk ami events to RabbitMQ",
    "keywords": ["rabbitmq", "asterisk", "pami", "ami", "amqp"],
    "license": "MIT",
    "authors": [
        {
            "name": "Richard Fullmer",
            "email": "richard.fullmer@opensoftdev.com"
        }
    ],
    "repositories": [
        {
            "type": "pear",
            "url": "http://pear.apache.org/log4php/"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/console": "2.1.*",
        "videlalvaro/php-amqplib": "dev-master",
        "marcelog/pami": "1.70.*"
    },
    "autoload": {
        "psr-0": {
            "Opensoft\\AsterBunny": "src/"
        }
    },
    "bin": [
        "bin/asterbunny"
    ]
}
